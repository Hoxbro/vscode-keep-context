{
  "name": "keep-context",
  "displayName": "Keep Context",
  "description": "Make context switch easy to be managed and tracked.",
  "icon": "https://raw.githubusercontent.com/marlomgirardi/vscode-keep-context/master/images/keep-context.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:keepContext",
    "onFileSystem:file"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "keep-context",
          "title": "Keep Context",
          "icon": "images/keep-context.svg"
        }
      ]
    },
    "views": {
      "keep-context": [
        {
          "id": "keepContext",
          "name": ""
        }
      ]
    },
    "commands": [],
    "menus": {}
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.7",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.2.0",
    "typescript": "^3.6.3",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.0"
  }
}
